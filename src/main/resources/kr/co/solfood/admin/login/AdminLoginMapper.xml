<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.solfood.admin.login.AdminLoginMapper">
    <select id="login" parameterType="String" resultType="kr.co.solfood.admin.login.AdminVO">
        SELECT *
        FROM admin
        WHERE admin_pwd = md5(#{adminPwd})
    </select>
</mapper>